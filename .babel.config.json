{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": "defaults",
				"corejs": 3.8,
				"useBuiltIns": "entry"
			}
		],
		"@babel/preset-react"
	],
	"plugins": [
		[
			"@babel/plugin-transform-runtime",
			{
				"corejs": 3.8,
				"helpers": true,
				"regenerator": true,
				"useESModules": true
			}
		]
	]
}
